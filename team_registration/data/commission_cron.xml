<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="ir_cron_commission_tracks_rollover" model="ir.cron" forcecreate="True">
            <field name="name">Commission Tracks Rollover</field>
            <field name="model_id" search="[('model', '=', 'user.commission.track')]"/>
            <field name="state">code</field>
            <field name="code">model.cron_process_commission_tracks()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="active" eval="True"/>
            <field name="user_id" ref="base.user_root"/>
        </record>

        <!-- Automatic Commission Top-up Cron -->
        <record id="ir_cron_commission_auto_topup" model="ir.cron" forcecreate="True">
            <field name="name">Commission: Automatic Wallet Top-up</field>
            <field name="model_id" search="[('model', '=', 'user.commission.track')]"/>
            <field name="state">code</field>
            <field name="code">model.cron_auto_commission_topup()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="active" eval="True"/>
            
        </record>
    </data>
</odoo>
