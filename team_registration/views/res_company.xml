<odoo>
    <record id="view_company_form_inherit" model="ir.ui.view">
        <field name="name">view.company.form.inherit</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form" />
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page name="team_flags" string="Team Flags">
                    <group name="team_flags" string="Visibility Options">
                        <field name="show_coach" />
                        <field name="show_team" />
                        <field name="loaylty_pointer_per_purchase_amount" />
                        <field name="discount_value" />
                        <field name="minimum_points_required" />
                        <field name="commission_cycle_days"></field>
                        <field name="test_today">2032-06-22</field>

                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>